- const { style } = data.character;
+blob({ name: 'style', title: 'Style' })
  +block('content')
    .style__track
      each radio in style.track
        label.style__checkbox
          +field(radio).checkbox--circle
          span.visually-hidden(data-i18n=`style_${radio.value}`)
    +span(style.subtitle)
